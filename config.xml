<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.test2478017" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Ionic HTML</name>
  <description>
        A minimal HTML editor that lets you test out ideas on the go.
    </description>
  <author email="contact@jerryzhou.net" href="jerryzhou.net">
      Jerry Zhou
  </author>
  <content src="index.html"/>
  <access origin="*"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="BackupWebStorage" value="none"/>
  <feature name="StatusBar">
    <param name="ios-package" value="CDVStatusBar" onload="true"/>
  </feature>
  <icon src="blackberry10/icon-86.png" />
  <platform name="ios">

        <!-- iOS 7.0+ -->
        <!-- iPhone / iPod Touch -->
        <icon src="icons/ios/icon-57.png" width="60" height="60" />
        <icon src="icons/ios/icon-57-2x.png" width="120" height="120" />

        <!-- iPad -->
        <icon src="icons/ios/icon-72.png" width="76" height="76" />
        <icon src="icons/ios/icon-72.png" width="152" height="152" />

        <!-- iOS 6.1 -->
        <!-- Spotlight Icon -->
        <icon src="icons/ios/icon-57.png" width="40" height="40" />
        <icon src="icons/ios/icon-57.png" width="80" height="80" />

        <!-- iPhone / iPod Touch -->
        <icon src="icons/ios/icon-57.png" width="57" height="57" />
        <icon src="icons/ios/icon-57-2x.png" width="114" height="114" />

        <!-- iPad -->
        <icon src="icons/ios/icon-72.png" width="72" height="72" />
        <icon src="icons/ios/icon-72-2x.png" width="144" height="144" />

        <!-- iPhone Spotlight and Settings Icon -->
        <icon src="icons/ios/icon-57.png" width="29" height="29" />
        <icon src="icons/ios/icon-57.png" width="58" height="58" />

        <!-- iPad Spotlight and Settings Icon -->
        <icon src="icons/ios/icon-72.png" width="50" height="50" />
        <icon src="icons/ios/icon-72.png" width="100" height="100" />
    </platform>
    <platform name="android">
        <icon src="icons/android/icon-96-xhdpi.png" />
    </platform>
</widget>
